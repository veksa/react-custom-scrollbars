{
  "name": "@veksa/custom-scrollbars",
  "version": "4.2.1-patch.1",
  "description": "React scrollbars component",
  "main": "./dist/cjs/scrollbars.cjs",
  "types": "./dist/scrollbars.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/scrollbars.d.ts",
      "import": "./dist/scrollbars.mjs",
      "default": "./dist/cjs/scrollbars.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "tsc --noEmit",
    "clean": "rimraf dist",
    "build": "npm run clean && tsup",
    "test": "cross-env NODE_ENV=test karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/malte-wessel/react-custom-scrollbars.git"
  },
  "keywords": [
    "scroll",
    "scroller",
    "scrollbars",
    "react-component",
    "react",
    "custom"
  ],
  "author": "Malte Wessel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/malte-wessel/react-custom-scrollbars/issues"
  },
  "homepage": "https://github.com/malte-wessel/react-custom-scrollbars",
  "dependencies": {
    "dom-css": "^2.1.0",
    "raf": "^3.4.1"
  },
  "devDependencies": {
    "@types/dom-css": "^2.1.1",
    "@types/raf": "3.4.3",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.0.1",
    "simulant": "^0.2.2",
    "tsup": "8.4.0",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "packageManager": "yarn@4.7.0"
}
